{
  "name": "myapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:gen:mysql": "prisma generate --schema=prisma/schema.prisma",

   
    "prisma:gen:mongo": "prisma generate --schema=prisma/mongo/schema.prisma",
      "db:deploy:mysql": "prisma migrate deploy",
    
    "postinstall": "npm run prisma:gen:mysql && npm run prisma:gen:mongo",

   
    "db:push:mongo": "prisma db push --schema=prisma/mongo/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "bcrypt": "^6.0.0",
    "cookie": "^1.0.2",
    "jsonwebtoken": "^9.0.2",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "prisma": "^6.14.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
